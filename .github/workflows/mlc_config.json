{
  "ignorePatterns": [
    { "pattern": "^https://github.com/.+/(compare|commit|pull)/" },
  { "pattern": "^https://github.com/.+/actions/workflows/.*" },
    { "pattern": "^https://img.shields.io/" },
  { "pattern": "^mailto:" },
  { "pattern": "^vscode://" }
  ],
  "httpHeaders": [
    { "urls": ["https://*"], "headers": { "User-Agent": "Mozilla/5.0" } }
  ],
  "retryOn429": true,
  "checkModifiedFilesOnly": false,
  "replacementPatterns": [
    { "pattern": "^#", "replacement": "" }
  ]
}
